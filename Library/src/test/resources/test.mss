{
  "pdf": {
    "extFonts": [
      {
        "family": "TestFont",
        "encoding": "ISO-8859-1",
        "path": "/fonts/ttf/test_font.ttf"
      },
      {
        "family": "Raleway-Regular",
        "encoding": "UTF-8",
        "path": "Library/src/test/resources/raleway/Raleway-Regular.ttf"
      },
      {
        "family": "Tuffy",
        "encoding": "UTF-8",
        "path": "Library/src/test/resources/tuffy/Tuffy.ttf"
      }
    ]
  },
  "colors": {
    "white": "255:255:255",
    "black": "0:0:0",
    "grey": "128:128:128",
    "blue": "0:0:180"
  },
  "document": {
    "pageFormat": "A4",
    "leftMargin": "2.5cm",
    "rightMargin": "2.5cm",
    "topMargin": "2cm",
    "bottomMargin": "2cm",

    "color": "0:0:0",
    "background": "ff:ff:ff",
    "family": "HELVETICA",
    "size": 11,
    "style": "Normal",

    "#": "Allows for the section number to be a little bit higher or lower than the rest of the line.",
    "sectionNumberYOffset": 2.0,

    "#": "Allows for the section number to have a different right margin than other text.",
    "sectionNumberXOffset": -10.0,

    "pages": {
      "image": {
        "imgScalePercent": 50.0,
        "imgAlign": "LEFT",
        "imgRotateDegrees": 0.0,
        "imgFlow": false,
        "imgFlowMargin": 4.0
      },
      "block_quote": {
        "family": "HELVETICA",
        "size": 11,
        "style": "ITALIC",
        "color": "128:128:128",
        "background": "white"
      },
      "h1": {
        "family": "HELVETICA",
        "size": 20,
        "style": "BOLD"
      },
      "h2": {
        "family": "HELVETICA",
        "size": 18,
        "style": "BOLD,UNDERLINE"
      },
      "h3": {
        "family": "Raleway-Regular",
        "size": 16,
        "style": "BOLD"
      },
      "h4": {
        "family": "Tuffy",
        "size": 14,
        "style": "BOLD"
      },
      "h5": {
        "family": "HELVETICA",
        "size": 12,
        "style": "BOLD"
      },
      "h6": {
        "family": "HELVETICA",
        "style": "BOLD"
      },
      "emphasis": {
        "family": "HELVETICA",
        "size": 10,
        "style": "ITALIC"
      },
      "strong": {
        "family": "HELVETICA",
        "size": 10,
        "style": "BOLD"
      },
      "code": {
        "family": "COURIER",
        "size": 8,
        "style": "NORMAL",
        "color": "64:64:64",
        "background": "#f8f8f8",
        "boxed": true,
        "boxedColor": "#f8f8f8"
      },
      "anchor": {
        "family": "HELVETICA",
        "size": 10,
        "style": "NORMAL",
        "color": "128:128:128",
        "background": "white"
      },
      "list_item": {
        "family": "HELVETICA",
        "size": 10
      },
      "footer": {
        "family": "HELVETICA",
        "size": 8
      },
      "horizontal_ruler": {
        "thickness": 0.5,
        "color": "blue"
      }
    },
    "divs": {
      "qaz": {
        "family": "COURIER",
        "color": "20:30:40",
        "background": "110:111:112",
        "size": 15,
        "style": "underline",
        "block_quote": {
          "family": "COURIER",
          "color": "120:120:120",
          "background": "10:11:12"
        }
      },
      "rat": {
        "image": {
          "imgFlow": true
        }
      }
    }
  },
  "front_page": {
    "color": "0:0:0",
    "background": "255:255:255",
    "family": "HELVETICA",
    "size": 10,
    "style": "NORMAL",
    "image": {
      "imgScalePercent": 60.0
    },
    "title": {
      "family": "COURIER",
      "size": 25,
      "style": "UNDERLINE"
    },
    "subject": {
      "family": "HELVETICA",
      "size": 15,
      "style": "NORMAL"
    },
    "version": {
      "family": "HELVETICA",
      "size": 12,
      "style": "NORMAL"
    },
    "copyright": {
    },
    "author": {
      "family": "HELVETICA",
      "size": 12,
      "style": "NORMAL"
    }
  },
  "toc": {
    "color": "0:0:0",
    "background": "255:255:255",
    "family": "HELVETICA",
    "size": 10,
    "style": "NORMAL",
    "h1": {
      "family": "HELVETICA",
      "size": 10,
      "style": "NORMAL"
    },
    "h2": {
      "family": "HELVETICA",
      "size": 10,
      "style": "NORMAL"
    },
    "h3": {
      "family": "HELVETICA",
      "size": 10,
      "style": "NORMAL"
    },
    "h4": {
      "family": "HELVETICA",
      "size": 10,
      "style": "NORMAL"
    },
    "h5": {
      "family": "HELVETICA",
      "size": 10,
      "style": "NORMAL"
    },
    "h6": {
      "family": "HELVETICA",
      "size": 10,
      "style": "NORMAL"
    }
  }
}

